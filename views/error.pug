doctype html
head
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1')
  // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
  title Error page
  // Google font
  link(href='https://fonts.googleapis.com/css?family=Quicksand:700' rel='stylesheet')
  // Font Awesome Icon
  link(type='text/css' rel='stylesheet' href='/css/vendor/font-awesome.min.css')
  // Custom stlylesheet
  link(type='text/css' rel='stylesheet' href='/css/vendor/style.css')
  // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
  // WARNING: Respond.js doesn't work if you view the page via file://
  //if lt IE 9
    script(src='https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js')
    script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
#notfound
  .notfound
    .notfound-bg
      div
      div
      div
    h1 oops!
    h2 Error #{statusCode} : #{env === "development" ? errorMessage : "Sorry, Your request couldn't handled."}
    a(href='#') go back
    .notfound-social
      a(href='#')
        i.fa.fa-facebook
      a(href='#')
        i.fa.fa-twitter
      a(href='#')
        i.fa.fa-pinterest
      a(href='#')
        i.fa.fa-google-plus

